generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("postgresql://postgres:root@localhost:5432/super_db?schema=public")
}

model TestResult {
  id        String   @id @default(uuid())
  username  String
  httpUrl   String
  wsUrl     String
  success   Boolean
  createdAt DateTime @default(now())
}
